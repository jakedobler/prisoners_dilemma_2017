        if getting_team_name:
            return 'GotGrapes?'
        else:
            if len(opponent_history)==0:
                return 'c'
            elif opponent_history[-1] == 'c':
               if opponent_history[-2] == 'c':
                    return 'b'
               else:
                    return random.choice(c, b)
            else:
                return 'b'

